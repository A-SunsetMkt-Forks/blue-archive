{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build:json": "node ./CICD/build_json.mjs",
    "build": "pnpm build:json && vite build && cp ./dist/index.html ./dist/404.html",
    "build:github": "pnpm build:json && vite build --base=/blue-archive/ && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build:json && vite preview --host",
    "check": "eslint --fix --config ./.eslintrc.json ./src/ ./CICD/ ./public/config/yaml/ --ext .js,.ts,.vue,.yml,.yaml && vue-tsc --noEmit --skipLibCheck",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.4.27",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.7.2",
    "axios-retry": "^4.3.0",
    "ba-story-player": "workspace:*",
    "fastest-levenshtein": "^1.0.16",
    "gsap": "^3.12.5",
    "husky": "^9.0.11",
    "ismobilejs": "^1.1.1",
    "mitt": "^3.0.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "pinyin-pro": "^3.20.4",
    "typed.js": "^2.1.0",
    "vue": "^3.4.29",
    "vue-router": "~4.3.0",
    "xxhashjs": "^0.2.2",
    "tailwindcss": "~3.4.1",
    "autoprefixer": "~10.4.17",
    "dayjs": "~1.11.11",
    "element-plus": "^2.7.3"
  },
  "devDependencies": {
    "taze": "^0.13.8",
    "@babel/preset-env": "^7.24.5",
    "@csstools/css-parser-algorithms": "^2.6.3",
    "@csstools/css-tokenizer": "^2.3.1",
    "@types/node": "^20.12.12",
    "@types/toastify-js": "^1.12.3",
    "@types/xxhashjs": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-legacy": "^5.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "chalk": "^5.3.0",
    "eslint": "9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-yaml": "^0.5.0",
    "js-yaml": "^4.1.0",
    "postcss": "~8.4.35",
    "postcss-plugin-px2rem": "^0.8.1",
    "postcss-preset-env": "^9.5.13",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.2",
    "terser": "^5.31.0",
    "typescript": "^5.4.3",
    "vite": "^5.2.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.20.0",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.19",
    "workbox-window": "^7.1.0",
    "ba-rig": "workspace:*"
  }
}
