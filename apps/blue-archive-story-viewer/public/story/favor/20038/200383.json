      "BGName": 1786002905,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#title;추운 나라에서 온 비서실장",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2003803,
      "SelectionGroup": 0,
