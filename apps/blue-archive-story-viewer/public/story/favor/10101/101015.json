      "VoiceId": 0
    },
    {
      "TextJp": "（サオリに連絡をもらい、会場のDJステージにやって来ていた）",
      "TextCn": "（我收到纱织的消息后，前往了会场的DJ舞台）",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010105,
      "SelectionGroup": 0,
      "BGMId": 0,
