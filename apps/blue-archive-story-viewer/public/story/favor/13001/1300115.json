      "GroupId": 1300115,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;치세;01;다른 풍령보다 더 파란 소리가 나니까.",
      "VoiceId": 0
    },
