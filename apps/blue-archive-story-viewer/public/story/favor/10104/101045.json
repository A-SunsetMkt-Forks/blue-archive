      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;레이죠;99;(지금 나에게 느껴지는 감정, 그것은…….)",
      "VoiceId": 0
    },
    {
      "TextJp": "（……怒り）",
      "TextCn": "（……愤怒）",
      "TextKr": "",
      "TextEn": "",
