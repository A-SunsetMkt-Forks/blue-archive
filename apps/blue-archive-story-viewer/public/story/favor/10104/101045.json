      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s] \"어…… 그, 그래. 쿵후야!\"",
      "VoiceId": 0
    },
    {
