      "BGName": 1047754314,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2003605,
      "SelectionGroup": 0,
