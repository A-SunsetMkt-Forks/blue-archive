      "ScriptKr": "3;모에 수영복;12\n#3;em;…\n#wait;2500",
      "VoiceId": 0
    },
    {
      "TextJp": "くひひっ……。",
      "TextCn": "咕嘻嘻……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1009702,
      "SelectionGroup": 0,
      "BGMId": 0,
