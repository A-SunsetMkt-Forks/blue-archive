  "proofreader": "今天早睡了吗",
  "content": [
    {
      "TextJp": "防護",
      "TextCn": "防卫",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001903,
      "SelectionGroup": 0,
      "BGMId": 16,
      "Sound": "",
