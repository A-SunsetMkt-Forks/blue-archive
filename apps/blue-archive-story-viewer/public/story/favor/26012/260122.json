      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s11] \"헉, 헉…….\"\n[s12] \"키리노, 괜찮아?\"",
      "VoiceId": 0
